<div class="card shadow mb-4 mt-2">
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>No</th>
                        <th>Title</th>
                        <th>Price</th>
                        <th>Country</th>
                        <th>City</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    <%   items.forEach( (item,index)=> { %>
                    <tr>
                        <td><%= index + 1 %></td>
                        <td><%= item.title %></td>
                        <td><%= item.price %></td>
                        <td><%= item.country %></td>
                        <td><%= item.city %></td>
                        <td>
                            <a href="/admin/item/images/<%= item.id  %>" class="btn btn-success btn-circle btn-sm"><i
                                    class="fas fa-images"></i></a>
                            <a href="javascript:void()" class="btn btn-success btn-circle btn-sm"><i
                                    class="fas fa-plus-circle"></i></a>
                            <a href="javascript:void()" class="btn btn-warning btn-circle btn-sm"><i
                                    class="fas fa-edit"></i></a>

                            <a href="javascript:void()" class="btn btn-danger btn-circle btn-sm"><i
                                    class="fas fa-trash"></i></a>
                        </td>
                    </tr>
                    <%  })  %>

                </tbody>
            </table>
        </div>
    </div>
</div>